
--CONSULTA USUARIOS CONFIGURADOS POR CGE
select *,
CONVERT(VARCHAR(250), DECRYPTBYPASSPHRASE('KIDFAROCADENJK',adm_bindato))
 from sysweb_adm.swe.SWTADM_CONFIGURACION
where CGE_IntCodigo in (
SELECT     CGE_IntCodigo
FROM         swe.SWTEMP_EMPRESA
where emp_chrestado = 'INC'
)
and adm_chrconfiguracion = 'CGEConcUsuarios'


