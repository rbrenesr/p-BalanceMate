--Actualizar cantidad de usuarios

--UPDATE SYSWEB_ADM.swe.SWTADM_CONFIGURACION
--SET ADM_BinDato = ENCRYPTBYPASSPHRASE('KIDFAROCADENJK','3')


select CONVERT(VARCHAR(250), DECRYPTBYPASSPHRASE('KIDFAROCADENJK',ADM_BinDato)),* from  SYSWEB_ADM.swe.SWTADM_CONFIGURACION

WHERE
	CGE_IntCodigo = 331 --- OJO CAMBIAR CODIGO CGE  
	AND ADM_ChrConfiguracion = 'CGEConcUsuarios'